    "@types/node": "^8.10.21",
    "@types/text-encoding": "0.0.33",
    "deep-diff": "^1.0.2",
    "lodash.flowright": "^3.5.0",
    "minimist": "^1.2.0",
    "prettier": "^1.13.4",
    "rimraf": "^2.6.2",
    "typescript": "^2.9.1"
